{
    "builds": [{
        "src": "tco/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "tco/wsgi.py"
        }
        
    ],
    "ignore": [
        "venv/",
        "__pycache__/",
        "*.pyc",
        "*.pyo",
        "*.log"
    ]
}